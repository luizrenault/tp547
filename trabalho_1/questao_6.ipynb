        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "\n",
        "N=1000000\n",
        "p=0.6\n",
        "x=np.random.uniform(0,1,N)\n",
        "value=6\n",
        "Xgeo=np.floor(np.log(x)/np.log(1-p)) + 1\n",
        "\n",
        "count=sum(Xgeo==value)\n",
        "print(Xgeo)\n",
        "\n",
        "prob=count/N\n",
        "print(\"a probabilidade e\",prob)\n",
        "\n",
        "plt.show()"
      ],
